{
  "parser": "babel-eslint",
  "rules": {
    "keyword-spacing": 2
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "React": true,
    "ReactDOM": true,
    "dataLayer": true,
    "Routes": true
  }
}
